<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat
    android:orientation="vertical"
    android:background="@drawable/plash2"
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="@dimen/_150sdp"
        android:layout_height="@dimen/_30sdp"
        android:layout_gravity="center"
        android:layout_marginTop="@dimen/_4sdp"
        android:gravity="center"
        >
        <androidx.appcompat.widget.AppCompatImageView
            android:layout_width="@dimen/_30sdp"
            android:layout_height="match_parent"
            android:background="@drawable/dorin"
            android:id="@+id/share_btn"
            android:src="@drawable/ic_share_black_24dp"
            android:layout_marginStart="@dimen/_15sdp"
            />
        <androidx.appcompat.widget.AppCompatImageView
            android:layout_width="@dimen/_30sdp"
            android:layout_height="match_parent"
            android:background="@drawable/dorin"
            android:id="@+id/val_btn"
            android:src="@drawable/ic_star_black_24dp"
            android:layout_marginStart="@dimen/_15sdp"
            />

    </androidx.appcompat.widget.LinearLayoutCompat>
    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:layout_height="match_parent">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/_110sdp"
            android:layout_marginTop="@dimen/_10sdp">

            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="@dimen/_90sdp"
                android:layout_height="@dimen/_110sdp"
                android:layout_gravity="start|center"
                android:layout_marginStart="@dimen/_20sdp"
                android:background="@drawable/elisker"
                android:id="@+id/listenazkarbtn" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/listenbtn"
                android:layout_width="108dp"
                android:layout_height="@dimen/_110sdp"
                android:layout_gravity="center"
                android:layout_marginStart="@dimen/_5sdp"
                android:background="@drawable/song3"

                />

            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="@dimen/_90sdp"
                android:layout_height="@dimen/_110sdp"
                android:layout_gravity="end|center"
                android:layout_marginRight="@dimen/_10sdp"
                android:id="@+id/readbtn"
                android:background="@drawable/qerain"

                />


        </FrameLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/_110sdp"
            android:layout_marginTop="@dimen/_10sdp"

            >

            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="@dimen/_90sdp"
                android:layout_height="@dimen/_110sdp"
                android:layout_gravity="start|center"
                android:layout_marginStart="@dimen/_20sdp"
                android:id="@+id/salat_btn"
                android:background="@drawable/moket_slai"

                />

            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="@dimen/_90sdp"
                android:layout_height="@dimen/_110sdp"
                android:layout_gravity="center"
                android:id="@+id/qibla_btn"

                android:layout_marginStart="@dimen/_5sdp"
                android:background="@drawable/qbla_atgey"

                />

            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="@dimen/_90sdp"
                android:layout_height="@dimen/_110sdp"
                android:layout_gravity="end|center"
                android:id="@+id/electronic_btn"
                android:layout_marginRight="@dimen/_10sdp"
                android:background="@drawable/sbhey"

                />


        </FrameLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/_110sdp"
            android:layout_marginTop="@dimen/_10sdp"

            >

            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="@dimen/_90sdp"
                android:layout_height="@dimen/_110sdp"
                android:layout_gravity="start|center"
                android:layout_marginStart="@dimen/_20sdp"
                android:background="@drawable/deia_khtm"
                android:id="@+id/doaa_btn"

                />

            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="@dimen/_90sdp"
                android:layout_height="@dimen/_110sdp"
                android:layout_gravity="center"
                android:id="@+id/btn_setting"
                android:layout_marginStart="@dimen/_5sdp"
                android:background="@drawable/setting_alarem"

                />

            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="@dimen/_90sdp"
                android:layout_height="@dimen/_110sdp"
                android:layout_gravity="end|center"
                android:id="@+id/nearest_mosque_btn"
                android:layout_marginRight="@dimen/_10sdp"
                android:background="@drawable/aqreb_masgd"

                />


        </FrameLayout>
        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/_110sdp"
            android:layout_marginTop="@dimen/_4sdp"

            >

            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="@dimen/_90sdp"
                android:id="@+id/sohor_alarm_btn"
                android:layout_marginRight="@dimen/_10sdp"

                android:layout_height="@dimen/_110sdp"
                android:layout_gravity="right"
                android:background="@drawable/alerm_shower"

                />
            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="@dimen/_90sdp"
                android:id="@+id/higri_btn"
                android:layout_height="@dimen/_110sdp"
                android:layout_marginStart="@dimen/_20sdp"


                android:layout_gravity="left"
                android:src="@drawable/higrical"

                />


        </FrameLayout>


    </androidx.appcompat.widget.LinearLayoutCompat>


</androidx.appcompat.widget.LinearLayoutCompat>